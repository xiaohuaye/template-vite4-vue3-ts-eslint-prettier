    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.34.5",
    "vue-tsc": "^1.8.4"
